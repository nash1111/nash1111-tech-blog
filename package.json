{
  "name": "nash1111-tech-blog",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "bun run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "bun run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "generate-task": "gh issue list --state all --json number,title,state,url,body > public/currentIssues.json && echo 'export const lastUpdated = \"'`date '+%Y-%m-%dT%H:%M:%S'`'\";' > public/lastUpdated.ts"
  },
  "dependencies": {
    "@babylonjs/core": "^7.54.3",
    "@babylonjs/loaders": "^7.54.3",
    "@mdx-js/rollup": "^3.1.0",
    "@playwright/test": "^1.51.1",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.6",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@remix-run/cloudflare": "^2.16.5",
    "@remix-run/cloudflare-pages": "^2.16.5",
    "@remix-run/react": "^2.16.5",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-table": "^8.21.2",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "deps-less-ui": "0.0.12",
    "feed": "^4.2.2",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "isbot": "^4.4.0",
    "lucide-react": "^0.372.0",
    "miniflare": "^3.20250408.0",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-pretty-code": "^0.13.2",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250412.0",
    "@remix-run/dev": "^2.16.5",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "node-fetch": "^3.3.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.18",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.114.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
